{
    "name": "backendtea/failuretest",
    "description": "Allow failing PHPUnit tests and keep a close eye on issues that have been fixed.",
    "keywords": ["php", "phpunit", "unit testing", "failures"],
    "type": "library",
    "support": {
        "issues": "https://github.com/BackEndTea/FailureTest/issues"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gert de Pagter",
            "email": "gertp93@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "FailureTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FailureTest\\Test\\": "tests/"
        }
    },
    "suggest": {
        "phpunit/phpunit": "Used to run tests"
    }
}
